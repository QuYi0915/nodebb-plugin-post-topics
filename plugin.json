{
	"id": "nodebb-plugin-post-topics",
	"name": "nodebb post topics",
	"description": "A plugin post topics for nodebb",
	"url": "https://github.com/QuYi0915/nodebb-plugin-post-topics.git",
	"library": "./lib.js",
	"hooks": [
		{
			"hook": "static:app.load", "method": "init"
		},
		{
